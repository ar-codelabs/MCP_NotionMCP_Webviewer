{
    "mcpServers": {
        "knowledge_base": {
            "command": "python3",
            "args": ["./application/mcp_server_retrieve.py"]
        },
        "repl_coder": {
            "command": "python3",
            "args": ["./application/mcp_server_repl_coder.py"]
        },
        "business_trip": {
            "command": "python3",
            "args": ["./application/mcp_server_business_trip.py"]
        },
        "awslabs.aws-documentation-mcp-server": {
            "command": "uvx",
            "args": ["awslabs.aws-documentation-mcp-server@latest"],
            "env": {
                "FASTMCP_LOG_LEVEL": "ERROR"
            }
        },
        "pdf_generator": {
            "command": "python3",
            "args": ["./application/mcp_server_pdf_generator.py"]
        },
        "notion": {
            "command": "python3",
            "args": ["./application/mcp_server_notion.py"],
            "env": {
                "NOTION_API_KEY": "your_notion_integration_token",
                "NOTION_PAGE_ID": "your_notion_page_id"
            }
        }
    }
}